#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x555dba168d00 .scope module, "testbench_adder" "testbench_adder" 2 2;
 .timescale -9 -12;
v0x555dba189e50_0 .var "a", 3 0;
v0x555dba189f30_0 .var "b", 3 0;
v0x555dba18a000_0 .var "c", 3 0;
v0x555dba18a100_0 .var "clk", 0 0;
v0x555dba18a1a0_0 .var "correct_ov", 0 0;
v0x555dba18a240_0 .var "correct_sum", 6 0;
v0x555dba18a320_0 .var "d", 3 0;
L_0x7fe0e1cd6138 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x555dba18a3e0_0 .net "ov", 0 0, L_0x7fe0e1cd6138;  1 drivers
v0x555dba18a4b0_0 .var "rst", 0 0;
v0x555dba18a550_0 .net "sum", 3 0, L_0x555dba18a950;  1 drivers
v0x555dba18a620_0 .var "test_num", 6 0;
E_0x555dba156120/0 .event negedge, v0x555dba18a4b0_0;
E_0x555dba156120/1 .event posedge, v0x555dba18a100_0;
E_0x555dba156120 .event/or E_0x555dba156120/0, E_0x555dba156120/1;
S_0x555dba168e80 .scope module, "DUT" "adder" 2 16, 3 1 0, S_0x555dba168d00;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /INPUT 4 "b"
    .port_info 2 /INPUT 4 "c"
    .port_info 3 /INPUT 4 "d"
    .port_info 4 /OUTPUT 4 "sum"
    .port_info 5 /OUTPUT 1 "ov"
L_0x7fe0e1cd6060 .functor BUFT 1, C4<000>, C4<0>, C4<0>, C4<0>;
v0x555dba1690a0_0 .net *"_s12", 2 0, L_0x7fe0e1cd6060;  1 drivers
v0x555dba188d40_0 .net *"_s13", 6 0, L_0x555dba18ace0;  1 drivers
v0x555dba188e20_0 .net *"_s15", 6 0, L_0x555dba18ae20;  1 drivers
L_0x7fe0e1cd60a8 .functor BUFT 1, C4<000>, C4<0>, C4<0>, C4<0>;
v0x555dba188ee0_0 .net *"_s18", 2 0, L_0x7fe0e1cd60a8;  1 drivers
v0x555dba188fc0_0 .net *"_s19", 6 0, L_0x555dba18afb0;  1 drivers
v0x555dba1890f0_0 .net *"_s21", 6 0, L_0x555dba18b0f0;  1 drivers
L_0x7fe0e1cd60f0 .functor BUFT 1, C4<000>, C4<0>, C4<0>, C4<0>;
v0x555dba1891d0_0 .net *"_s24", 2 0, L_0x7fe0e1cd60f0;  1 drivers
v0x555dba1892b0_0 .net *"_s25", 6 0, L_0x555dba18b240;  1 drivers
v0x555dba189390_0 .net *"_s5", 6 0, L_0x555dba18aa70;  1 drivers
L_0x7fe0e1cd6018 .functor BUFT 1, C4<000>, C4<0>, C4<0>, C4<0>;
v0x555dba189470_0 .net *"_s8", 2 0, L_0x7fe0e1cd6018;  1 drivers
v0x555dba189550_0 .net *"_s9", 6 0, L_0x555dba18ab60;  1 drivers
v0x555dba189630_0 .net "a", 3 0, v0x555dba189e50_0;  1 drivers
v0x555dba189710_0 .net "b", 3 0, v0x555dba189f30_0;  1 drivers
v0x555dba1897f0_0 .net "c", 3 0, v0x555dba18a000_0;  1 drivers
v0x555dba1898d0_0 .net "d", 3 0, v0x555dba18a320_0;  1 drivers
v0x555dba1899b0_0 .net "ov", 0 0, L_0x7fe0e1cd6138;  alias, 1 drivers
v0x555dba189a70_0 .net "s1", 0 0, L_0x555dba18a6e0;  1 drivers
v0x555dba189b30_0 .net "s2", 0 0, L_0x555dba18a7e0;  1 drivers
v0x555dba189bf0_0 .net "s3", 0 0, L_0x555dba18a8b0;  1 drivers
v0x555dba189cb0_0 .net "sum", 3 0, L_0x555dba18a950;  alias, 1 drivers
L_0x555dba18a6e0 .part L_0x555dba18b240, 6, 1;
L_0x555dba18a7e0 .part L_0x555dba18b240, 5, 1;
L_0x555dba18a8b0 .part L_0x555dba18b240, 4, 1;
L_0x555dba18a950 .part L_0x555dba18b240, 0, 4;
L_0x555dba18aa70 .concat [ 4 3 0 0], v0x555dba189e50_0, L_0x7fe0e1cd6018;
L_0x555dba18ab60 .concat [ 4 3 0 0], v0x555dba189f30_0, L_0x7fe0e1cd6060;
L_0x555dba18ace0 .arith/sum 7, L_0x555dba18aa70, L_0x555dba18ab60;
L_0x555dba18ae20 .concat [ 4 3 0 0], v0x555dba18a000_0, L_0x7fe0e1cd60a8;
L_0x555dba18afb0 .arith/sum 7, L_0x555dba18ace0, L_0x555dba18ae20;
L_0x555dba18b0f0 .concat [ 4 3 0 0], v0x555dba18a320_0, L_0x7fe0e1cd60f0;
L_0x555dba18b240 .arith/sum 7, L_0x555dba18afb0, L_0x555dba18b0f0;
    .scope S_0x555dba168d00;
T_0 ;
    %delay 5000, 0;
    %load/vec4 v0x555dba18a100_0;
    %inv;
    %store/vec4 v0x555dba18a100_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x555dba168d00;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x555dba18a4b0_0;
    %inv;
    %store/vec4 v0x555dba18a4b0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x555dba168d00;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555dba18a100_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x555dba18a4b0_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x555dba189e50_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x555dba189f30_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x555dba18a000_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x555dba18a320_0, 0;
    %pushi/vec4 0, 0, 7;
    %assign/vec4 v0x555dba18a620_0, 0;
    %vpi_call 2 27 "$dumpfile", "adder.vcd" {0 0 0};
    %vpi_call 2 28 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x555dba168d00;
T_3 ;
    %wait E_0x555dba156120;
    %load/vec4 v0x555dba18a4b0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x555dba189e50_0;
    %pad/u 32;
    %cmpi/u 15, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_3.2, 5;
    %load/vec4 v0x555dba189e50_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x555dba189e50_0, 0;
    %load/vec4 v0x555dba18a620_0;
    %addi 1, 0, 7;
    %assign/vec4 v0x555dba18a620_0, 0;
T_3.2 ;
    %load/vec4 v0x555dba189e50_0;
    %pad/u 32;
    %cmpi/e 15, 0, 32;
    %jmp/0xz  T_3.4, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x555dba189e50_0, 0;
    %load/vec4 v0x555dba189f30_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x555dba189f30_0, 0;
T_3.4 ;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x555dba189e50_0;
    %pad/u 8;
    %load/vec4 v0x555dba189f30_0;
    %pad/u 8;
    %add;
    %split/vec4 7;
    %store/vec4 v0x555dba18a240_0, 0, 7;
    %store/vec4 v0x555dba18a1a0_0, 0, 1;
    %load/vec4 v0x555dba18a3e0_0;
    %load/vec4 v0x555dba18a550_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 8;
    %load/vec4 v0x555dba18a1a0_0;
    %load/vec4 v0x555dba18a240_0;
    %concat/vec4; draw_concat_vec4
    %cmp/e;
    %jmp/0xz  T_3.6, 4;
    %vpi_call 2 46 "$display", "Test %d ", v0x555dba18a620_0 {0 0 0};
    %vpi_call 2 47 "$display", "OK!" {0 0 0};
    %jmp T_3.7;
T_3.6 ;
    %vpi_call 2 56 "$display", "Test %d ", v0x555dba18a620_0 {0 0 0};
    %vpi_call 2 57 "$display", "/////////////////////" {0 0 0};
    %vpi_call 2 58 "$display", "////////Fail!////////" {0 0 0};
    %vpi_call 2 59 "$display", "/////////////////////" {0 0 0};
    %vpi_call 2 60 "$display", "%d + %d + %d + %d = ?", v0x555dba189e50_0, v0x555dba189f30_0, v0x555dba18a000_0, v0x555dba18a320_0 {0 0 0};
    %vpi_call 2 61 "$display", "your answer: ov = %d, sum = %d", v0x555dba18a3e0_0, v0x555dba18a550_0 {0 0 0};
    %vpi_call 2 62 "$display", "correct answer: ov = %d, sum = %d", v0x555dba18a1a0_0, v0x555dba18a240_0 {0 0 0};
    %vpi_call 2 63 "$display", "\012" {0 0 0};
T_3.7 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x555dba168d00;
T_4 ;
    %delay 1005000, 0;
    %vpi_call 2 67 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
