#! /usr/bin/vvp
:ivl_version "10.1 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x55cdebde8330 .scope module, "testbench_add2" "testbench_add2" 2 2;
 .timescale -9 -12;
v0x55cdebe08730_0 .var "a", 3 0;
v0x55cdebe08810_0 .var "b", 3 0;
v0x55cdebe088e0_0 .var "clk", 0 0;
v0x55cdebe089b0_0 .var "correct_ov", 0 0;
v0x55cdebe08a50_0 .var "correct_sum", 3 0;
L_0x7f31dbc58018 .functor BUFT 1, C4<0>, C4<0>, C4<0>, C4<0>;
v0x55cdebe08b80_0 .net "ov", 0 0, L_0x7f31dbc58018;  1 drivers
v0x55cdebe08c20_0 .var "rst", 0 0;
v0x55cdebe08cc0_0 .net "sum", 3 0, L_0x55cdebe08e70;  1 drivers
v0x55cdebe08db0_0 .var "test_num", 6 0;
E_0x55cdebdda4c0/0 .event negedge, v0x55cdebe08c20_0;
E_0x55cdebdda4c0/1 .event posedge, v0x55cdebe088e0_0;
E_0x55cdebdda4c0 .event/or E_0x55cdebdda4c0/0, E_0x55cdebdda4c0/1;
S_0x55cdebde84b0 .scope module, "DUT" "add2" 2 16, 3 1 0, S_0x55cdebde8330;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "a"
    .port_info 1 /INPUT 4 "b"
    .port_info 2 /OUTPUT 4 "sum"
    .port_info 3 /OUTPUT 1 "ov"
v0x55cdebde8630_0 .net "a", 3 0, v0x55cdebe08730_0;  1 drivers
v0x55cdebe083f0_0 .net "b", 3 0, v0x55cdebe08810_0;  1 drivers
v0x55cdebe084d0_0 .net "ov", 0 0, L_0x7f31dbc58018;  alias, 1 drivers
v0x55cdebe085a0_0 .net "sum", 3 0, L_0x55cdebe08e70;  alias, 1 drivers
L_0x55cdebe08e70 .arith/sum 4, v0x55cdebe08730_0, v0x55cdebe08810_0;
    .scope S_0x55cdebde8330;
T_0 ;
    %delay 5000, 0;
    %load/vec4 v0x55cdebe088e0_0;
    %inv;
    %store/vec4 v0x55cdebe088e0_0, 0, 1;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55cdebde8330;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x55cdebe08c20_0;
    %inv;
    %store/vec4 v0x55cdebe08c20_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55cdebde8330;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cdebe088e0_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55cdebe08c20_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55cdebe08730_0, 0;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55cdebe08810_0, 0;
    %pushi/vec4 0, 0, 7;
    %assign/vec4 v0x55cdebe08db0_0, 0;
    %vpi_call 2 25 "$dumpfile", "add2.vcd" {0 0 0};
    %vpi_call 2 26 "$dumpvars" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55cdebde8330;
T_3 ;
    %wait E_0x55cdebdda4c0;
    %load/vec4 v0x55cdebe08c20_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_3.0, 8;
    %load/vec4 v0x55cdebe08730_0;
    %pad/u 32;
    %cmpi/u 15, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz  T_3.2, 5;
    %load/vec4 v0x55cdebe08730_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55cdebe08730_0, 0;
    %load/vec4 v0x55cdebe08db0_0;
    %addi 1, 0, 7;
    %assign/vec4 v0x55cdebe08db0_0, 0;
T_3.2 ;
    %load/vec4 v0x55cdebe08730_0;
    %pad/u 32;
    %cmpi/e 15, 0, 32;
    %jmp/0xz  T_3.4, 4;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x55cdebe08730_0, 0;
    %load/vec4 v0x55cdebe08810_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x55cdebe08810_0, 0;
T_3.4 ;
    %jmp T_3.1;
T_3.0 ;
    %load/vec4 v0x55cdebe08730_0;
    %pad/u 5;
    %load/vec4 v0x55cdebe08810_0;
    %pad/u 5;
    %add;
    %split/vec4 4;
    %store/vec4 v0x55cdebe08a50_0, 0, 4;
    %store/vec4 v0x55cdebe089b0_0, 0, 1;
    %load/vec4 v0x55cdebe08b80_0;
    %load/vec4 v0x55cdebe08cc0_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55cdebe089b0_0;
    %load/vec4 v0x55cdebe08a50_0;
    %concat/vec4; draw_concat_vec4
    %cmp/e;
    %jmp/0xz  T_3.6, 4;
    %vpi_call 2 44 "$display", "Test %d ", v0x55cdebe08db0_0 {0 0 0};
    %vpi_call 2 45 "$display", "OK!" {0 0 0};
    %vpi_call 2 46 "$display", "%d + %d = ?", v0x55cdebe08730_0, v0x55cdebe08810_0 {0 0 0};
    %vpi_call 2 47 "$display", "your answer: ov = %d, sum = %d", v0x55cdebe08b80_0, v0x55cdebe08cc0_0 {0 0 0};
    %vpi_call 2 48 "$display", "correct answer: ov = %d, sum = %d", v0x55cdebe08b80_0, v0x55cdebe08cc0_0 {0 0 0};
    %vpi_call 2 49 "$display", "\012" {0 0 0};
    %jmp T_3.7;
T_3.6 ;
    %vpi_call 2 52 "$display", "Test %d ", v0x55cdebe08db0_0 {0 0 0};
    %vpi_call 2 53 "$display", "/////////////////////" {0 0 0};
    %vpi_call 2 54 "$display", "////////Fail!////////" {0 0 0};
    %vpi_call 2 55 "$display", "/////////////////////" {0 0 0};
    %vpi_call 2 56 "$display", "%d + %d = ?", v0x55cdebe08730_0, v0x55cdebe08810_0 {0 0 0};
    %vpi_call 2 57 "$display", "your answer: ov = %d, sum = %d", v0x55cdebe08b80_0, v0x55cdebe08cc0_0 {0 0 0};
    %vpi_call 2 58 "$display", "correct answer: ov = %d, sum = %d", v0x55cdebe089b0_0, v0x55cdebe08a50_0 {0 0 0};
    %vpi_call 2 59 "$display", "\012" {0 0 0};
T_3.7 ;
T_3.1 ;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55cdebde8330;
T_4 ;
    %delay 1005000, 0;
    %vpi_call 2 63 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
